<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/join"
        tools:context=".HomePage">
    <Button
            android:text="Manufacturer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/manufacturer" tools:layout_editor_absoluteY="455dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="72dp"
            android:layout_marginStart="72dp" tools:ignore="MissingConstraints"/>
    <Button
            android:text="Vendor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/vender" tools:layout_editor_absoluteY="455dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="52dp"
            android:layout_marginRight="52dp" tools:ignore="MissingConstraints"/>
</androidx.constraintlayout.widget.ConstraintLayout>